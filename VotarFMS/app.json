{
  "expo": {
    "name": "Votar FMS",
    "slug": "votarfms-reactnative",
    "version": "4.0.0",
    "orientation": "portrait",
    "scheme": "votarfms",
    "userInterfaceStyle": "light",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#b80e0e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.eddd.votarfms"
    },
    "android": {
      "package": "com.eddd.votarfms"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3984939241340358~4212688543",
          "iosAppId": "ca-app-pub-3984939241340358~4212688543"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "votar-fms",
          "organization": "mappek"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "922254f6-db13-4989-9847-4f66b5da54a3"
      }
    }
  }
}
